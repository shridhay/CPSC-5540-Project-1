program tesseractedsum 
  pre n >= 0 
  post p = ((((n * n) + n) * ((2 * n) + 1) * ((3 * n * n) + (3 * n) - 1)) / 30)
  is
  p := 0;
  x := 0;
  while x < n
    inv p = ((((x * x) + x) * ((2 * x) + 1) * ((3 * x * x) + (3 * x) - 1)) / 30)
    inv x <= n 
  do
    x := x + 1;
    p := p + (x * x * x * x);
